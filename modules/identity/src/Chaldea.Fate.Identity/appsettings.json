{
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:9001"
      }
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "IdentityServer4": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext} {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext} {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "App": {
    "CorsOrigins": "https://*.chaldea.cn,http://localhost:4200",
    "Clock": {
      "Kind": 1
    }
  },
  "ConnectionStrings": {
    "Default": "mongodb://root:1qaz!QAZ@192.168.50.151:27017/chaldea?authMechanism=SCRAM-SHA-256&authSource=admin"
  },
  "AuthServer": {
    "Authority": "http://192.168.50.151:58491/core",
    "ClientId": "platform",
    "ClientSecret": "platform"
  }
}